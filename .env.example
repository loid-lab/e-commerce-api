# PostgreSQL connection string (Neon or local)
DB_URL=postgres://user:password@host:5432/dbname

# JWT secret for signing tokens (use a long, random string)
SECRET=your_jwt_secret_here

# Stripe secret API key (for payment integration)
STRIPE_SECRET_KEY=sk_test_your_stripe_key_here

# Redis URL (Docker: use redis:6379; Local: use localhost:6379)
REDIS_URL=redis://redis:6379